/* 公共数据  */
.modules {
  color : #4a4a4a;
}
.modules .price {
  margin : 0;
  line-height: 42rpx;
  font-size : 42rpx;
  color : #df393c;
  display: inline;
}
.modules .price .sub {
  margin-right : 10rpx;
  display: inline;
  font-size : 28rpx;
}
.modules .origin-price {
  display: inline;
  margin : 0;
  text-decoration: line-through;
  font-size: 24rpx;
  color : #ccc;
}
.modules .plus-icon {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size : 32rpx;
  height : 48rpx;
  width : 48rpx;
  border-radius: 50%;
  background-color: #161823;
  color : #fff;
}
/* 搜索模块  */
.modules .search {
  padding : 20rpx;
}
.modules .search .input {
  display: flex;
  height : 60rpx;
  align-items: center;
  padding : 0 20rpx;
  border-radius: 30rpx;
  background-color: #eee;
  color : #aaa;
}
.modules .search .input image {
  width : 32rpx;
  height : 32rpx;
  margin-right : 20rpx;
}
/* 1n  */
.modules .n1 {
  border-top : solid 1px #f0f0f0;
}
.modules .n1 .item {
  padding : 20rpx;
  display: flex;
  border-bottom : solid 1px #f0f0f0;
}
.modules .n1 .item .pic {
  width : 240rpx;
  height : 240rpx;
  position: relative;
}
.modules .n1 .item .pic .tags {
  position : absolute;
  left : 10rpx;
  top : 10rpx;
  padding : 6rpx 16rpx;
  border-radius: 50rpx;
  background-color: #09BB07;
  color : #fff;
  font-size : 20rpx;
}
.modules .n1 .item .pic image {
  width : 100%;
  height : 100%;
}
.modules .n1 .item .info {
  margin-left : 20rpx;
  padding : 20rpx 0;
  width : 450rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.modules .n1 .item .info .title {
  font-size : 32rpx;
  line-height: 38rpx;
}
.modules .n1 .item .info .sub-title {
  font-size : 26rpx;
  line-height: 32rpx;
  color : #9B9B9B;
}
.modules .n1 .item .info .origin-price {
  margin-left: 20rpx;
}
.modules .n1 .item .info .info-act {
  position: relative;
  height : 42rpx;
}
.modules .n1 .item .info .info-act .add {
  background-color: #161823;
  color : #fff;
  border-radius: 8rpx;
  padding : 0 20rpx;
  height : 50rpx;
  line-height: 50rpx;
  font-size : 28rpx;
  position: absolute;
  right : 0;
  top : 0;
}
/* 2n  */
