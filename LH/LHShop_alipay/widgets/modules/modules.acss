.modules {
  width : 100%;
  overflow: hidden;
  padding-bottom: 100rpx;
}
.modules .header {
  position: fixed;
  top : 0;
  left : 0;
  width : 100%;
  z-index : 1;
  background-color: #fff;
}
.modules .header .tab {
  display: flex;
  border-bottom: solid 1px #f0f0f0;
}
.modules .header .tab view {
  color : #404040;
  flex-grow : 1;
  text-align: center;
  height : 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
}
.modules .header .tab view.current {
  color : #f5692e;
}
.modules .slider,
.modules .slider swiper {
  width : 100%;
  height : 350rpx;
}
.modules .slider image {
  width : 100%;
  height : 350rpx;
}
.modules .entry {
  display: flex;
  width : 100%;
  padding : 10rpx 0;
  margin-top : 0;
}
.modules .entry view {
  padding : 10rpx;
  flex-grow : 1;
  text-align: center;
  font-size : 24rpx;
  color : #888;
}
.modules .entry view image {
  height : 80rpx;
  width : 80rpx;
  display: block;
  margin : 0 auto;
  margin-bottom : 10rpx;
}
.modules .banner image {
  width : 100%;
  height : 240rpx;
}
.modules .normal-image {
  line-height: 0;
}
.modules .normal-image,
.modules .normal-image image {
  width: 100%;
}
/* 店铺街  */
.modules .m-6-9 {
  padding : 10rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.modules .m-6-9 .shops {
  width : 562rpx;
  display: flex;
  justify-content: space-between;
}
.modules .m-6-9 .shops .shop {
  font-size : 20rpx;
  padding : 0 20rpx;
  width : 134rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.modules .m-6-9 .shops .shop .logo {
  width : 114rpx;
  height : 114rpx;
}
.modules .m-6-9 .shops .shop .name {
  text-align: center;
  width : 134rpx;
  margin-top : 20rpx;
}
.modules .m-6-9 .more {
  display: none;
  width : 167rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size : 24rpx;
  color : #999;
}
.modules .m-6-9 .more .big {
  margin-bottom : 10rpx;
  font-size : 36rpx;
}
.modules .m-6-9 .empty {
  width : 100%;
  height: 134rpx;
  color : #999;
  display: flex;
  justify-content: center;
  align-items: center;
}
/* 九宫格  */
.modules .module-1 {
  display: flex;
  flex-wrap: wrap;
  border-top : solid 1px #eee;
}
.modules .module-1.flex-col .col {
  width : 33.33%;
}
.modules .module-1 .item {
  position: relative;
  left : 1px;
  padding : 14rpx;
  border-right : solid 1px #eee;
  border-bottom : solid 1px #eee;
}
.modules .module-1 .item .image {
  width : 210rpx;
  height : 210rpx;
}
.modules .module-1 .item .title {
  display : flex;
  width : 210rpx;
  height : 60rpx;
  line-height: 30rpx;
  font-size : 26rpx;
  overflow: hidden;
  color : #404040;
  margin-top : 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.modules .module-1 .item .price {
  margin-top : 10rpx;
}
.modules .module-1 .item .price .last {
  display: inline-block;
  height : 28rpx;
  line-height: 28rpx;
  overflow : hidden;
  color : #999;
  font-size : 24rpx;
  position: relative;
}
.modules .module-1 .item .price .last view {
  width : 100%;
  border-bottom: solid 1px;
  position: absolute;
  top : 14rpx;
}
.modules .module-1 .item .price .current {
  font-size : 26rpx;
  height: 28rpx;
  line-height: 28rpx;
  color : #f5692e;
}
.modules .module-1 .item .add-cart {
  position: absolute;
  right : 14rpx;
  bottom : 5rpx;
}
/* 表格输入相关  */
.modules .m-row {
  display: flex;
  align-items: flex-start;
  flex-direction: column;
  padding : 10rpx 30rpx 20rpx 30rpx;
  border-bottom : solid 1px #f9f9f9;
}
.modules .m-row .m-key {
  padding : 10rpx 0;
  display: flex;
  position: relative;
  width : 100%;
  align-items: center;
  font-size : 30rpx;
}
.modules .m-row .m-key.must {
  padding-left : 20rpx;
}
.modules .m-row .m-key.must:before {
  content : '*';
  color : red;
  position: absolute;
  left : 0rpx;
}
.modules .m-row .m-key:after {
  content : ':';
  margin-left : 10rpx;
}
.modules .m-row .m-value {
  width : 100%;
  flex-grow: 1;
  display: flex;
}
.modules .m-row .m-value input,
.modules .m-row .m-value textarea,
.modules .m-row .m-value .picker {
  display: block;
  padding : 0 10rpx;
  flex-grow: 1;
  border : solid 1px #f0f0f0;
  border-radius: 8rpx;
}
.modules .m-row .m-value input {
  height : 60rpx;
}
.modules .m-row .m-value .picker {
  height: 60rpx;
  line-height: 60rpx;
  background-image : linear-gradient(0deg,#f4f4f4,#fff);
  padding-right : 20rpx;
  position: relative;
  overflow: hidden;
}
.modules .m-row .m-value .picker:after {
  content : '';
  width: 0;
  height: 0;
  border-left: 4px solid transparent;
  border-right: 4px solid transparent;
  border-top: 6px solid #888;
  position: absolute;
  right : 10rpx;
  top : 24rpx;
}
.modules .m-row .m-value .picker picker {
  width : 100%;
  height: 100%;
}
.modules .m-row .m-value textarea {
  height : 120rpx;
}
.modules .m-row .m-value input.input-hidden {
  display: none;
}
.modules .module-2 {
  margin-bottom : 10px;
  height : 440rpx;
}
.modules .module-2 .row {
  height : 220rpx;
}
.modules .module-2 image,
.modules .module-2 navigator {
  display: block;
  width : auto;
  height : 100%;
}
.modules .module-2 navigator {
  border-bottom : solid 1px #eee;
  border-right : solid 1px #eee;
  border-top : solid 1px #eee;
}
.modules .module-3 {
  display: flex;
}
.modules .module-3 image,
.modules .module-3 navigator {
  width : 100%;
  height : 100%;
}
.modules .module-4 {
  height : 300rpx;
}
.modules .module-4 image,
.modules .module-4 navigator {
  width : 100%;
  height : 100%;
}
