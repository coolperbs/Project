<!--pages/signUpB/signUpB.wxml-->
<view class='sign-up-input'>
  <view class="input-box">
    <image class="input-icon" src="../../images/image_phone@2x.png"></image>
    <view class="chose-area">+86</view>
    <input type="number" placeholder-class="input-placeholder" class="phone" placeholder='输入您的手机号码'></input>
  </view>
  <view class="input-box">
    <image class="input-icon" src="../../images/image_reg_code@2x.png"></image>
    <input type="number" placeholder-class="input-placeholder" class="phone" placeholder='输入您的手机号码'></input>
    <view class="get-code">获取验证码</view>
  </view>
</view>
<button class="sign-up-btn">
下一步
</button>