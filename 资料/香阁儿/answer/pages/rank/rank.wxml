							</view>
							<view class="bottom">
								<view class="text">奖金:80</view>
								<view class="star-list">
									<view class="star">
										<image class="start-box" src="../../static/images/star-bg.png"></image>
										<image class="start-active" src="../../static/images/star.png"></image>
									</view>
									<view class="star active">
										<image class="start-box" src="../../static/images/star-bg.png"></image>
										<image class="start-active" src="../../static/images/star.png"></image>
