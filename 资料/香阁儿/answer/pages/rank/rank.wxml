						<image class="item-image" src="../../static/images/lv1.png"></image>
						<view class="desc">
							<view class="top">
								<view class="title">入门萌新</view>
								<view class="coin">
									<image class="coin-icon" src="../../static/images/coin.png"></image>
									<view class="coin-count">20</view>
								</view>
							</view>
							<view class="bottom">
								<view class="text">奖金:80</view>
								<view class="star-list">
									<view class="star">
										<image class="start-box" src="../../static/images/star-bg.png"></image>
										<image class="start-active" src="../../static/images/star.png"></image>
									</view>
									<view class="star active">
										<image class="start-box" src="../../static/images/star-bg.png"></image>
										<image class="start-active" src="../../static/images/star.png"></image>
									</view>
								</view>
							</view>
						</view>
					</view>
				</button>
				<button hover-class="level-item-hover" class="level-item">
					<view class="item-box">
						<image class="item-image" src="../../static/images/lv1.png"></image>
