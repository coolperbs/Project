<view class="page-CPApply">
	<view class="page-CPApply-wrapper form-wrapper">
		<view class="form-header">
			<view class="logo"></view>
			基本信息
		</view>
		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>姓名</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>性别</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value">
					<picker bindchange="bindPickerChange" value="0" range="['男','女']">
					    <view class="picker-value">
					      	男
					    </view>
					 </picker>
				</view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>入党申请时间</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>


		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>民族</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>身份证号</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>自愿注册党组织</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>联系电话</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>籍贯</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>职业</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>所属行业</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>学历</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>毕业院校</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>有何专长</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>现居住地</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><input data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>需要说明其他问题</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value"><textarea data-key="phone" catchTap="changeInput"/></view>
			</view>
		</view>

		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>入党申请表</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value">
					<textarea data-key="phone" catchTap="changeInput"/>
				</view>
			</view>
		</view>


		<view class="form-content disabled">
			<view class="field {{inValidField.indexOf('name')>0?'inValid':''}}">
				<view class="name">
					<text>两寸照片</text>
					<text class="notBlank">*</text>
				</view>
				<view class="value">
					<textarea data-key="phone" catchTap="changeInput"/>
					<view class="notice">友情提示:只允许上传两寸(长度宽度为90*140至100*150像素)的红底照片</view>
				</view>
			</view>
		</view>

	</view>
	<view class="btn-wrapper btns">
		<view catchTap="submit btn" class="submit btn" data-type="submit">确认</view>
		<view catchTap="cancel btn" class="cancel btn" data-type="cancel">取消</view>
	</view>
</view>