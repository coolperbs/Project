<div class="w-w-login">
	<div class="pop">
		<div class="close J_Close">x</div>
		<div class="nav">
			<div class="tab J_Tab" data-type="Login">登录</div>
			<div class="tab">|</div>
			<div class="tab J_Tab" data-type="Register">注册</div>
		</div>
		<div class="J_Cont">
		</div>
		<script class="J_RegisterTemp" type="text/html">
			<div class="form register">
				<form class="J_RegisterForm">
					<div class="row">
						<div class="cell">
							<div class="key">手机号</div>
							<div class="value"><input name="userPhone"/></div>
						</div>
					</div>
					<div class="row">
						<div class="cell">
							<div class="key">身份证号</div>
							<div class="value"><input name="idCard"/></div>
						</div>
					</div>
					<div class="row">
						<div class="cell">
							<div class="key">密码</div>
							<div class="value"><input type="password" name="password"/></div>
						</div>
					</div>
					<div class="row">
						<div class="cell">
							<div class="key">再次输入密码</div>
							<div class="value"><input type="password" name="cheackPassword"/></div>
						</div>
					</div>
					<div class="row msg J_Error">
						<div class="cell">
							<div class="key"></div>
							<div class="value"></div>
						</div>
					</div>
				</form>
			</div>
			<div class="act">
				<div class="btn J_Register">注册</div>
			</div>				
		</script>
		<script class="J_LoginTemp" type="text/html">
			<div class="form">
				<form class="J_LoginForm">
					<div class="row">
						<div class="cell">
							<div class="key">手机号</div>
							<div class="value"><input name="userPhone" /></div>
						</div>
					</div>
					<div class="row">
						<div class="cell">
							<div class="key">身份证号</div>
							<div class="value"><input name="idCard"/></div>
						</div>
					</div>
					<div class="row">
						<div class="cell">
							<div class="key">密码</div>
							<div class="value"><input type="password" name="password"/></div>
						</div>
					</div>
					<div class="row msg J_Error">
						<div class="cell">
							<div class="key">*</div>
							<div class="value"></div>
						</div>
					</div>
				</form>
			</div>
			<div class="act">
				<div class="btn J_Login">登录</div>
			</div>
		</script>
	</div>
</div>