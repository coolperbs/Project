<div class="w-p-sub-apply">
	<div class="head">
		<div class="title">在线入党</div>
		<div class="steps">
			<div class="step 1"><em>1</em>. 学习党章</div>
			<div class="arrow">></div>
			<div class="step 2"><em>2</em>. 登记申请</div>
		</div>
	</div>
	<div class="cont J_Cont">
	</div>

	<div class="act form"> 
		<div class="btn primary J_Next">完成阅读</div>
		<div class="btn primary J_Submit">提交</div>
	</div>	
	<script type="text/html" class="J_Note">
		<div class="d-title">党章</div>
		<div>党章信息</div>
	</script>


	<script type="text/html" class="J_ContTemp">
		<div class="form">
			<form class="J_Form">
			<div class="row">
				<div class="cell">
					<div class="key">姓名</div>
					<div class="value">
						<input name="userName" data-name="姓名" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">联系电话</div>
					<div class="value">
						<input name="userPhone" data-name="联系电话" type="text"/>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">身份证号</div>
					<div class="value">
						<input name="idCart" data-name="身份证号" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">民族</div>
					<div class="value">
						<input name="nation" data-name="民族" type="text"/>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">籍贯</div>
					<div class="value">
						<input name="placeOrigin" data-name="籍贯" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">挂靠组织</div>
					<div class="value">
						<select name="organization" data-name="挂靠组织">
							<option value="第一支部">第一支部</option>
							<option value="第一支部">第二支部</option>
							<option value="第一支部">第三支部</option>
							<option value="第一支部">第四支部</option>
							<option value="第一支部">第五支部</option>
							<option value="第一支部">第六支部</option>
						</select>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">性别</div>
					<div class="value">
						<select name="gender" data-name="性别">
							<option value="男">男</option>
							<option value="女">女</option>
						</select>
					</div>
				</div>
				<div class="cell">
					<div class="key">毕业学校</div>
					<div class="value">
						<input name="school" data-name="毕业学校" type="text"/>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">学历</div>
					<div class="value">
						<input name="education" data-name="学籍" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">有何专长</div>
					<div class="value">
						<input name="goodat" data-name="专长" type="text"/>
					</div>
				</div>			
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">行业</div>
					<div class="value">
						<input name="industry" data-name="行业" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">职业</div>
					<div class="value">
						<input name="occupation" data-name="职业" type="text"/>
					</div>
				</div>				
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">现居住地</div>
					<div class="value">
						<input name="livingPlace" data-name="现居住地" type="text"/>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">备注</div>
					<div class="value">
						<textarea rows="3" name="otherInfo"></textarea>
					</div>
				</div>
			</div>
			</form>
		</div>
	</script>
</div>