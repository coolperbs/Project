<div class="w-p-sub-change">
	<div class="head">
		<div class="title">找党组织</div>
	</div>
	<div class="cont J_Cont">
	</div>
	<div class="act form"> 
		<div class="btn primary J_Submit">提交</div>
	</div>
	<script class="J_ContTemp" type="text/html">
		<div class="form">
			<form class="J_Form">
			<div class="row">
				<div class="cell">
					<div class="key">姓名</div>
					<div class="value">
						<input name="userName" data-name="姓名" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">身份证号</div>
					<div class="value">
						<input name="idCard" data-name="身份证号" type="text"/>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">手机号</div>
					<div class="value">
						<input maxlength="11" data-name="手机号" name="userPhone" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">挂靠组织</div><!-- 一到六支部 -->
					<div class="value">
						<select name="anchoredOrganization">
							<option value="第一支部">第一支部</option>
							<option value="第二支部">第二支部</option>
							<option value="第三支部">第三支部</option>
							<option value="第四支部">第四支部</option>
							<option value="第五支部">第五支部</option>
							<option value="第六支部">第六支部</option>
							<option value="第七支部">第七支部</option>
						</select>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">流动类型</div>
					<div class="value">
						<select name="transferType">
							<option value="市内流动">市内流动</option>
							<option value="省内跨市流动">省内跨市流动</option>
							<option value="跨省流动">跨省流动</option>
						</select>
					</div>
				</div>
				<div class="cell">
					<div class="key">流动原因</div>
					<div class="value">
						<input name="transferReason" data-name="流动原因" type="text"/>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">原职业</div>
					<div class="value">
						<input name="originalOccupation" data-name="原职业" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">原党组织</div>
					<div class="value">
						<input name="originalOrganization" data-name="原党组织" type="text"/>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">原联系人</div>
					<div class="value">
						<input name="originalLinkman" data-name="原联系人" type="text"/>
					</div>
				</div>
				<div class="cell">
					<div class="key">原联系方式</div>
					<div class="value">
						<input name="originalContact"  data-name="原联系方式" type="text"/>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="cell">
					<div class="key">备注</div>
					<div class="value">
						<textarea name="note"  data-name="备注" rows="3"></textarea>
					</div>
				</div>
			</div>
			</form>
		</div>
	</script>
</div>